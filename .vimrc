syntax on
map fg 
set history=700
filetype plugin on
filetype indent on
set autoread
set wildmenu
set wildignore=*.o,*~,*.pyc
set ruler
set ignorecase
set magic
set showmatch
set noerrorbells
syntax enable
colorscheme desert
set background=dark
set noswapfile
set shiftwidth=4
set ai
set si
set nu
set nocompatible
set tabstop=4
set showcmd
set autoindent
set smarttab
set smartcase
set mouse=a
inoremap jj <Esc>
